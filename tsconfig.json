{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "jsx": "preserve",
    "allowJs": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
  
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "suppressImplicitAnyIndexErrors": true,
 
    "skipLibCheck": true,
    "strict": true,
    "declaration": true,
   
    "baseUrl": "./"
  }
}
